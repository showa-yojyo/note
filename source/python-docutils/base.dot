digraph G {
    graph [
        fontname = "Bitstream Vera Sans"
        fontsize = 9
    ]
    node [
        fontname = "Bitstream Vera Sans"
        fontsize = 9
        shape = "record"
    ]

    SettingsSpec [
        label = "{SettingsSpec
        |settings_spec = ()\lsettings_defaults = None\lsettings_default_overrides = None\lrelative_path_settings = ()\lconfig_section = None\lconfig_section_dependencies = None\l
        |}"
    ]
    TransformSpec [
        label = "{TransformSpec
        |
        |+ get_transforms()\l}"
    ]
    Component [
        label = "{Component
        |component_type = None\lsupported = ()\l
        |+ supports(str) : bool\l}"
    ]

    edge [
        arrowhead = "empty"
    ]
    Component -> TransformSpec, SettingsSpec

    { rank = min; SettingsSpec, TransformSpec }
    { rank = max; Component }
}
